@WebService(serviceName = "ComprobarUsuario")
public class ComprobarUsuario() {

    @WebMethod(operationName = "Comprobar")
    public boolean Comprobar(@WebParam(name = "usuario") String user1, 
                             @WebParam(name = "contrasenia") String contra) {
        try {
            if (user1 == "usuario" && contrasenia == contra) {
                return true;
            } else if (user1 == "usuario" && contrasenia != contra) {
                return false;
            } else if (user1 != "usuario" && contrasenia != contra) {
                return false;
            }
        } catch (Exception e) {
            return false;
        }
        return false;
    }
}
